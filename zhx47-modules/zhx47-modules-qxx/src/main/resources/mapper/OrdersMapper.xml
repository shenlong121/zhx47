<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.zhx47.qxx.mapper.OrdersMapper">
    <select id="queryByUserId" resultType="top.zhx47.qxx.datasource.entity.Orders" parameterType="java.lang.String">
        SELECT *
        FROM `orders`
        WHERE user_id = #{userId}
    </select>
</mapper>